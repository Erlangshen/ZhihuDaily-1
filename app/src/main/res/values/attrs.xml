<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="border_overlay" format="boolean"/>
        <attr name="fill_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ColorIndicatorView">
        <attr name="showCheck" format="boolean"/>
        <attr name="colorValue" format="color"/>
        <attr name="colorName" format="string"/>
        <attr name="colorNameSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="AutoScrollViewPager">
        <attr format="boolean" name="indeterminate"/>
        <attr format="boolean" name="autoScroll"/>
        <attr format="integer" name="duration"/>
    </declare-styleable>
</resources>